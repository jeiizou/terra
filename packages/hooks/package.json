{
  "name": "terra-hook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "node ./esbuild.run.cjs && tsc --emitDeclarationOnly --outDir dist",
    "build:watch": "node ./esbuild.run.cjs && tsc --emitDeclarationOnly --outDir dist",
    "start": "cross-env WATCH=true concurrently -n es,tcs -c blue,red \"node ./esbuild.run.cjs\" \"tsc --emitDeclarationOnly --outDir dist --watch\""
  },
  "dependencies": {
    "lodash": "^4.14.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.5",
    "npm-dts": "^1.3.12",
    "typescript": "^4.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
